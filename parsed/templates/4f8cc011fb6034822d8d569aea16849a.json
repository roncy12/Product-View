{"components/page/checkout/popup-delivery":"<style>\r\n    .container-popup-checkout{\r\n        width:100%;\r\n        position:relative;\r\n        background-color:#666666;\r\n        right:-100%;\r\n        opacity: 0;\r\n        padding:13px;display:flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n       \r\n    }\r\n    .image-container-popup{\r\n        width:10%;\r\n    }\r\n    .container-delivery-popup-blurb{\r\n        width:90%;\r\n        padding-left:17px;\r\n    }\r\n    .container-delivery-popup-blurb p i{\r\n        color:#d22229;\r\n    }\r\n    .container-delivery-popup-blurb p{\r\n        margin:0;\r\n        color:white;\r\n    }\r\n</style>\r\n<script>\r\n$(document).ready(function(){\r\n\r\n\r\n    var checkExist = setInterval(function() {\r\n        if ($('span[data-test=\"cart-price-value\"]').length) {\r\n            var html=`\r\n            <div class=\"container-popup-checkout\">\r\n                <div class=\"image-container-popup\">\r\n                    <img class=\"lazyload\" src=\"https://cdn11.bigcommerce.com/s-bek4r4rsz5/product_images/uploaded_images/truck-icon-delivery.png?t=1643342227&_gl=1*j961sn*_ga*MTY3NzY3MzYuMTYzNDgwNDgwOA..*_ga_WS2VZYPC6G*MTY0Mzc2Mjc3NC4yMy4xLjE2NDM3NjI4NTIuNDI.\" />\r\n                </div>\r\n                <div class=\"container-delivery-popup-blurb\">\r\n                    <p>\r\n                        <i class=\"fa fa-info-circle\"></i>\r\n                        Local delivery generally available within a few days. We will call you to set your delivery\r\n                        within 24-hours of your purchase.\r\n                    </p>\r\n                </div>\r\n            </div>\r\n            `;\r\n            $('.layout-cart').append(html);\r\n            setTimeout(function(){\r\n                $('.container-popup-checkout').animate({'right':'0px','opacity':'1'},1000,function(){\r\n                    setTimeout(function(){\r\n                        $('.container-popup-checkout').animate({'right':'-200%','opacity': '0'},500);\r\n                    },8000);\r\n                });\r\n            },1000);\r\n            clearInterval(checkExist);\r\n        }\r\n    }, 100); // check every 100ms\r\n\r\n\r\n\r\n});\r\n</script>\n","layout/_footer-script":"\t\t<button onclick=\"topFunction()\" id=\"myBtn\" title=\"Go to top\">\r\n\t\t\t<span class=\"material-icons\">arrow_upward</span>\r\n\t\t</button>\r\n\r\n\t\t<script>// When the user scrolls down 20px from the top of the document, show the button\r\n\t\twindow.onscroll = function() {\r\n\t\t  scrollFunction()\r\n\t\t};\r\n\t\t\t\t\t\r\n\t\tvar navbar = document.getElementById(\"stick-navbar\");\r\n\t\tvar sticky = navbar.offsetTop;\r\n\t\t\t\t\t\r\n\t\tfunction scrollFunction() {\r\n\t\t  if (document.body.scrollTop > 300 || document.documentElement.scrollTop > 300) {\r\n\t\t\tdocument.getElementById(\"myBtn\").style.display = \"block\";\r\n\t\t\tnavbar.classList.add(\"sticky\")\r\n\t\t  } else {\r\n\t\t\tdocument.getElementById(\"myBtn\").style.display = \"none\";\r\n\t\t\tnavbar.classList.remove(\"sticky\");\r\n\t\t  }\r\n\t\t}\r\n\r\n\t\t// When the user clicks on the button, scroll to the top of the document\r\n\t\tfunction topFunction() {\r\n\t\t  document.body.scrollTop = 0;\r\n\t\t  document.documentElement.scrollTop = 0;\r\n\t\t}\r\n\t\t</script>\r\n\r\n\r\n        <script>window.__webpack_public_path__ = \"{{cdn 'assets/dist/'}}\";</script>\r\n        <script src=\"{{cdn 'assets/dist/theme-bundle.main.js'}}\"></script>\r\n        <script>\r\n            {{!-- Exported in app.js --}}\r\n            window.stencilBootstrap(\"{{page_type}}\", {{jsContext}}).load();\r\n        </script>\r\n        \r\n\r\n\r\n        {{{footer.scripts}}}\r\n","layout/_head-script":"\r\n\t\t<link href=\"https://fonts.googleapis.com/icon?family=Material+Icons\"rel=\"stylesheet\">\r\n\t\t<link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css\">\r\n\t  \r\n        <!--<script src=\"https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js\" integrity=\"sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==\" crossorigin=\"anonymous\" referrerpolicy=\"no-referrer\" async></script>-->\r\n        <script src=\"https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js\"></script>\r\n        \r\n\t\t<link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.css\" integrity=\"sha512-wR4oNhLBHf7smjy0K4oqzdWumd+r5/+6QO/vDda76MW5iug4PT7v86FoEkySIJft3XA0Ae6axhIvHrqwm793Nw==\" crossorigin=\"anonymous\" referrerpolicy=\"no-referrer\" />\t  \r\n\t<!-- product page bootstrap conflict and product page stamped unknown script  -->\r\n\t{{#if template_file '==' 'pages/product' }}\r\n\t\t<script> \r\n\t\t\tfunction myInit(){     \r\n\t\t\tStampedFn.init({ \r\n\t\t\tapiKey: 'pubkey-611zwH64k90x30Y02Agj8b89pH7MW7', // replace include # (hash)\r\n\t\t\tstoreUrl: 'kings34.mybigcommerce.com' // replace include # (hash), without https:// (e.g. www.domain.com)\r\n\t\t\t});\r\n\t\t\t} \r\n\t\t</script> \r\n    \r\n\t\t<script async onload=\"myInit()\" type=\"text/javascript\" src=\"https://cdn1.stamped.io/files/widget.min.js\"></script>\r\n\t\t\r\n\t{{else}}\r\n\t\t<link href=\"https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css\" rel=\"stylesheet\">\r\n\t\t<link rel=\"stylesheet\" href= \"https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css\" >\r\n        <script src=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js\" async></script>\t\r\n\t\t\r\n\t{{/if}}\r\n\t \r\n        <script type=\"text/javascript\" charset=\"utf8\" src=\"https://cdn.datatables.net/1.10.21/js/jquery.dataTables.min.js\" async></script>\r\n\t\t<link rel=\"stylesheet\" type=\"text/css\" href=\"https://cdn.datatables.net/1.10.21/css/jquery.dataTables.min.css\">\r\n\t  \r\n\t  \r\n\t   <!-- OTT ads -->\r\n       <img src=\"https://tags.w55c.net/rs?id=89e8788c3a3d43b4a248311021f87aea&t=marketing\" style=\"display:none;\" />\r\n\t   \r\n\t   \r\n        <script>\r\n            {{!-- Change document class from no-js to js so we can detect this in css --}}\r\n            document.documentElement.className = document.documentElement.className.replace('no-js', 'js');\r\n        </script>\r\n        {{!-- Load Lazysizes script ASAP so images will appear --}}\r\n        <script>\r\n            {{!-- Only load visible elements until the onload event fires, after which preload nearby elements. --}}\r\n            window.lazySizesConfig = window.lazySizesConfig || {};\r\n            window.lazySizesConfig.loadMode = 1;\r\n        </script>\r\n        <script async src=\"{{cdn 'assets/dist/theme-bundle.head_async.js'}}\"></script>\r\n\r\n        {{ getFontsCollection }}\r\n        {{{stylesheet '/assets/css/theme.css'}}}\r\n\r\n        {{{head.scripts}}}\r\n  \r\n        {{~inject 'themeSettings' theme_settings}}\r\n        {{~inject 'genericError' (lang 'common.generic_error')}}\r\n        {{~inject 'maintenanceMode' settings.maintenance}}\r\n        {{~inject 'urls' urls}}\r\n        {{~inject 'secureBaseUrl' settings.secure_base_url}}\r\n        {{~inject 'cartId' cart_id}}\r\n        {{~inject 'template' template}}\t   \r\n","layout/empty":"<!DOCTYPE html>\n<html>\n<head>\n    <title>{{ head.title }}</title>\n    {{{ head.meta_tags }}}\n    {{{ head.config }}}\n\n    <link href=\"{{ head.favicon }}\" rel=\"shortcut icon\">\n    {{#block \"head\"}}{{/block}}\n    \n\t{{> layout/_head-script }}\n\n</head>\n<body>\n    {{#block \"page\"}}{{/block}}\n\t\n\t{{> layout/_footer-script }}\n</body>\n</html>\n","pages/checkout":"{{#partial \"head\"}}\r\n\r\n{{{ checkout.checkout_head }}}\r\n{{{ stylesheet '/assets/css/optimized-checkout.css' }}}\r\n{{ getFontsCollection }}\r\n\r\n<script type=\"text/javascript\">\r\n    window.language = {{{langJson 'optimized_checkout'}}};\r\n</script>\r\n\r\n{{{head.scripts}}}\r\n\r\n{{/partial}}\r\n\r\n{{#partial \"page\"}}\r\n\r\n<header class=\"checkoutHeader optimizedCheckout-header\">\r\n    <div class=\"checkoutHeader-content\">\r\n        <h1 class=\"is-srOnly\">{{lang 'checkout.title'}}</h1>\r\n        <h2 class=\"checkoutHeader-heading\">\r\n            <a class=\"checkoutHeader-link\" href=\"{{urls.home}}\">\r\n                {{#if checkout.header_image}}\r\n                    <img alt=\"{{settings.store_logo.title}}\" class=\"checkoutHeader-logo\" id=\"logoImage\" src=\"{{ checkout.header_image }}\"/>\r\n                {{ else }}\r\n                    <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\r\n                {{/if}}\r\n            </a>\r\n        </h2>\r\n    </div>\r\n</header>\r\n\r\n{{{ checkout.checkout_content }}}\r\n\r\n\r\n{{{ footer.scripts }}}\r\n\r\n{{/partial}}\r\n\r\n{{> layout/empty}}\r\n\r\n{{#if theme_settings.show_popup '==' 'yes'}}\r\n{{>components/page/checkout/popup-delivery}}\r\n{{/if}}\r\n\r\n"}
